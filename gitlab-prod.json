{
  "id": "/prod/gitlab-prod",
  "backoffFactor": 1.15,
  "backoffSeconds": 1,
  "container": {
    "portMappings": [
      {
        "containerPort": 80,
        "hostPort": 0,
        "labels": {
          "VIP_0": "/gitlab:80"
        },
        "protocol": "tcp",
        "servicePort": 0,
        "name": "http"
      },
      {
        "containerPort": 443,
        "hostPort": 0,
        "labels": {
          "VIP_1": "/gitlab:443"
        },
        "protocol": "tcp",
        "servicePort": 0,
        "name": "https"
      },
      {
        "containerPort": 22,
        "hostPort": 0,
        "labels": {
          "VIP_2": "/gitlab:22"
        },
        "protocol": "tcp",
        "servicePort": 0,
        "name": "ssh"
      }
    ],
    "type": "DOCKER",
    "volumes": [
      {
        "containerPath": "/etc/gitlab",
        "hostPath": "/tmp/prod/gitlab_prod-config",
        "mode": "RW"
      },
      {
        "containerPath": "/var/log/gitlab",
        "hostPath": "/tmp/prod/gitlab_prod-logs",
        "mode": "RW"
      },
      {
        "containerPath": "/var/opt/gitlab",
        "hostPath": "/tmp/prod/gitlab_prod-data",
        "mode": "RW"
      }
    ],
    "docker": {
      "image": "gitlab/gitlab-ce:latest",
      "forcePullImage": true,
      "privileged": false,
      "parameters": []
    }
  },
  "cpus": 1,
  "disk": 0,
  "instances": 1,
  "maxLaunchDelaySeconds": 3600,
  "mem": 8192,
  "gpus": 0,
  "networks": [
    {
      "mode": "container/bridge"
    }
  ],
  "requirePorts": false,
  "upgradeStrategy": {
    "maximumOverCapacity": 1,
    "minimumHealthCapacity": 1
  },
  "killSelection": "YOUNGEST_FIRST",
  "unreachableStrategy": {
    "inactiveAfterSeconds": 0,
    "expungeAfterSeconds": 0
  },
  "healthChecks": [],
  "fetch": [],
  "constraints": []
}
