  "labels": {
    "HAPROXY_0_VHOST": "ext-ly-hub-499080470.us-west-2.elb.amazonaws.com",
    "HAPROXY_GROUP": "external",
    "HAPROXY_DEPLOYMENT_GROUP": "tweeter-awswest"
  },
  "id": "/tweeterapp/tweeter-awswest",
  "backoffFactor": 1.15,
  "backoffSeconds": 1,
  "cmd": "export KAFKA_BROKERS='broker.kafka-awswest.l4lb.thisdcos.directory:9092' && until rake cassandra:setup; do sleep 5; done && rails server --binding 0.0.0.0",
  "constraints": [
    [
      "@region",
      "IS",
      "us-west-2"
    ]
  ],
  "container": {
    "portMappings": [
      {
        "containerPort": 3000,
        "hostPort": 0,
        "labels": {
          "VIP_0": "2.2.2.2:30000"
        },
        "protocol": "tcp",
        "servicePort": 10000
      }
    ],
    "type": "MESOS",
    "volumes": [],
    "docker": {
      "image": "mesosphere/tweeter",
      "forcePullImage": true,
      "parameters": []
    }
